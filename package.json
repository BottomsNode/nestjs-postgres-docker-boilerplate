{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "A scalable NestJS boilerplate with libs, modules, TypeORM, and PostgreSQL",
  "author": "nestjs-boilerplate",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=development nest build",
    "lint": "eslint \"{src,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "docker:up:db": "cross-env NODE_ENV=docker docker compose -f docker-compose-development.yml --env-file src/environment/.env.docker up -d --build",
    "docker:down": "docker compose -f docker-compose-development.yml down -v",
    "docker:logs": "docker compose -f docker-compose-development.yml logs -f",
    "migration:generate": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/database/data-source.ts ./src/database/migrations",
    "migration:run": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/database/data-source.ts",
    "migration:revert": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/database/data-source.ts",
    "seed": "npx ts-node -r tsconfig-paths/register src/database/seeders/init-seed.ts"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nestjs-pino": "^4.4.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.27.11",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.8.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "^@modules(|/.*)$": "<rootDir>/src/modules/$1",
      "^@config(|/.*)$": "<rootDir>/src/config/$1",
      "^@seeders(|/.*)$": "<rootDir>/src/database/seeders/$1"
    }
  }
}
